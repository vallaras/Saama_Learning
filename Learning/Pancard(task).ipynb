    "import re\n",
    "def validcard(match):\n",
    "    reg='[A-Z]{5}[0-9]{4}[A-Z]{1}'\n",
    "    a=re.compile(reg)\n",
    "    if(re.search(a,match) and len(match)==10):\n",
    "        return True\n",
    "    else:\n",
    "        return False  \n",
    "validcard(input(\"Enter the PANCARDNO:\"))"
   ]
